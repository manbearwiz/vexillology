{
  "name": "vexillology",
  "version": "0.0.0-development",
  "description": "Pluggable feature flag SDK wrapper for Javascript",
  "files": ["dist"],
  "scripts": {
    "build": "npx concurrently npm:build:*",
    "build:vite": "vite build",
    "build:tsc": "tsc -b tsconfig.types.json",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "test:watch": "vitest --watch --coverage --ui",
    "prepublish": "npm run build",
    "semantic-release": "semantic-release",
    "lint": "npx @biomejs/biome check",
    "lint:fix": "npx @biomejs/biome check --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manbearwiz/vexillology.git"
  },
  "keywords": [],
  "author": "Kevin Brey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/manbearwiz/vexillology/issues"
  },
  "homepage": "https://github.com/manbearwiz/vexillology#readme",
  "peerDependencies": {
    "@optimizely/optimizely-sdk": "^4.0.0",
    "@splitsoftware/splitio": "^10.2.0",
    "launchdarkly-js-client-sdk": "^3.0.0",
    "unleash-proxy-client": "^3.3.1"
  },
  "peerDependenciesMeta": {
    "@optimizely/optimizely-sdk": {
      "optional": true
    },
    "@splitsoftware/splitio": {
      "optional": true
    },
    "launchdarkly-js-client-sdk": {
      "optional": true
    },
    "unleash-proxy-client": {
      "optional": true
    }
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-angular": "^19.3.0",
    "@tsconfig/recommended": "^1.0.3",
    "@tsconfig/strictest": "^2.0.2",
    "@tsconfig/vite-react": "^3.0.2",
    "@types/node": "^20.12.12",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "happy-dom": "^15.0.0",
    "semantic-release": "^22.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.3.4",
    "vitest": "^2.0.5"
  },
  "release": {
    "branches": ["main", { "name": "beta", "prerelease": true }]
  },
  "type": "module",
  "types": "dist/types/index.d.ts",
  "main": "./dist/vexillology.umd.cjs",
  "module": "./dist/vexillology.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/vexillology.js",
      "require": "./dist/vexillology.umd.cjs"
    }
  }
}
